#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules/.pnpm/tree-kill@1.2.2/node_modules/tree-kill/node_modules:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules/.pnpm/tree-kill@1.2.2/node_modules:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules/.pnpm/node_modules:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules:/mnt/c/Users/LENOVO/Documents/node_modules:/mnt/c/Users/LENOVO/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules/.pnpm/ts-node-dev@1.1.8/node_modules/ts-node-dev/node_modules:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules/.pnpm/ts-node-dev@1.1.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules/.pnpm/tree-kill@1.2.2/node_modules/tree-kill/node_modules:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules/.pnpm/tree-kill@1.2.2/node_modules:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules/.pnpm/node_modules:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules:/mnt/c/Users/LENOVO/Documents/node_modules:/mnt/c/Users/LENOVO/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules/.pnpm/ts-node-dev@1.1.8/node_modules/ts-node-dev/node_modules:/mnt/c/Users/LENOVO/Documents/EXPRESS/node_modules/.pnpm/ts-node-dev@1.1.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tree-kill@1.2.2/node_modules/tree-kill/cli.js" "$@"
else
  exec node  "$basedir/../../../../../tree-kill@1.2.2/node_modules/tree-kill/cli.js" "$@"
fi
